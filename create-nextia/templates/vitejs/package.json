{
	"name": "TEMPLATE",
	"description": "description",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"eslintConfig": {
		"extends": "./node_modules/standard/eslintrc.json"
	},
	"scripts": {
		"start": "vite --mode dev",
		"clean": "rm -fr .coverage node_modules out package-lock.json",
		"make": "npm run clean && npm install",
		"build": "vite build --mode",
		"preview": "vite preview",
		"lint": "standard 'src/**/*.{js,jsx}' 'test/**/*.{js,jsx}' --verbose",
		"page": "npm create nextia@latest page",
		"component": "npm create nextia@latest component",
		"container": "npm create nextia@latest container",
		"test": "vitest",
		"test:name": "vitest --testNamePattern",
		"test:silent": "vitest --silent",
		"test:coverage": "vitest --silent --coverage"
	},
	"devDependencies": {
		"@vitejs/plugin-react": "^5.0.0",
		"@vitest/coverage-v8": "^3.2.4",
		"autoprefixer": "^10.4.21",
		"jsdom": "^26.1.0",
		"vite": "^7.1.1",
		"vitest": "^3.0.4",
		"standard": "^17.1.2"
	},
	"dependencies": {
		"nextia": "^4.0.5",
		"react": "^19.1.1",
		"react-dom": "^19.1.1"
	}
}