		"test:silent": "vitest --silent",
		"test:coverage": "vitest --silent --coverage"
	},
	"devDependencies": {
		"@vitejs/plugin-react": "^4.7.0",
		"@vitest/coverage-v8": "^3.2.4",
		"autoprefixer": "^10.4.21",
		"jsdom": "^26.1.0",
		"vite": "^7.0.5",
		"vitest": "^3.0.4"
	},
	"dependencies": {
		"nextia": "file:../..",
		"react": "^19.1.0",
		"react-dom": "^19.1.0"
	}
