{
	"name": "vitejs",
	"description": "vitejs",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"start": "vite --mode development",
		"clean": "rm -fr .coverage node_modules out package-lock.json",
		"make": "npm run clean && npm install",
		"build": "vite build --mode",
		"preview": "vite preview",
